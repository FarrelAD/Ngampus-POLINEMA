package mulai.oop;

public class Mahasiswa {
	String nama;
	int umur;
	String nim;
	
	Mahasiswa(String nama, String nim) {
		this.nama = nama;
		this.nim = nim;
	}

	public String getNama() {
		return nama;
	}
	
	public String getNim() {
		return nim;
	}
	
	public int getUmur() {
		return umur;
	}
	
	public void setNama(String namaBaru) {
		nama = namaBaru;
	}
	
	public void setUmur(int umurBaru) {
		umur = umurBaru;
	}
	
	public void setNim(String nimBaru) {
		nim = nimBaru;
	}
}
